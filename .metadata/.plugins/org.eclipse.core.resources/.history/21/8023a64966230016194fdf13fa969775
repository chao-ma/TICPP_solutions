#include <iostream>
#include <string>
#include "stack2.h"

using namespace std;

int main() {
	string s[] = {"aa", "bb", "cc","abcde"};
	Stack stack;
	stack.initialize();
	cout << sizeof(s) << endl;
	cout << sizeof(s)/sizeof(string) << endl;
	stack.cleanup();
	return 0;
}



